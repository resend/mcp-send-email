{
  "mcpServers": {
    "resend": {
      "command": "node",
      "args": ["dist/index.js"],
      "env": {
        "RESEND_API_KEY": "${RESEND_API_KEY}"
      }
    },
    "resend-dev": {
      "command": "node",
      "args": ["dist/index.js", "--key", "${RESEND_API_KEY}"],
      "env": {}
    }
  }
}
